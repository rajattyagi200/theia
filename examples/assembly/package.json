{
  "private": true,
  "name": "@eclipse-che/theia-assembly",
  "version": "0.0.1",
  "theia": {
    "frontend": {
      "config": {
        "applicationName": "Eclipse Che",
        "defaultTheme": "dark",
        "defaultIconTheme": "theia-file-icons",
        "preferences": {
          "editor.autoSave": "on",
          "editor.semanticHighlighting.enabled": false,
          "git.defaultCloneDirectory": "/projects",
          "application.confirmExit": "always",
          "files.enableTrash": false
        }
      }
    }
  },
  "dependencies": {
    "@theia/callhierarchy": "^1.29.0",
    "@theia/console": "^1.29.0",
    "@theia/core": "^1.29.0",
    "@theia/debug": "^1.29.0",
    "@theia/editor": "^1.29.0",
    "@theia/file-search": "^1.29.0",
    "@theia/filesystem": "^1.29.0",
    "@theia/keymaps": "^1.29.0",
    "@theia/markers": "^1.29.0",
    "@theia/messages": "^1.29.0",
    "@theia/metrics": "^1.29.0",
    "@theia/mini-browser": "^1.29.0",
    "@theia/monaco": "^1.29.0",
    "@theia/navigator": "^1.29.0",
    "@theia/outline-view": "^1.29.0",
    "@theia/output": "^1.29.0",
    "@theia/plugin-dev": "^1.29.0",
    "@theia/plugin-ext": "^1.29.0",
    "@theia/plugin-ext-vscode": "^1.29.0",
    "@theia/preferences": "^1.29.0",
    "@theia/preview": "^1.29.0",
    "@theia/process": "^1.29.0",
    "@theia/search-in-workspace": "^1.29.0",
    "@theia/task": "^1.29.0",
    "@theia/userstorage": "^1.29.0",
    "@theia/variable-resolver": "^1.29.0",
    "@theia/workspace": "^1.29.0",
    "babel-loader": "^8.2.2",
    "buffer": "^6.0.3",
    "source-map-loader": "^2.0.1",
    "source-map-support": "^0.5.19",
    "style-loader": "^2.0.0",
    "umd-compat-loader": "^2.1.2",
    "webpack": "^5.36.2",
    "worker-loader": "^3.0.8",
    "async-limiter": "^2.0.0",
    "@eclipse-che/theia-remote": "0.0.1",
    "@eclipse-che/plugin": "0.0.1",
    "@eclipse-che/theia-plugin-ext": "0.0.1",
    "@eclipse-che/theia-user-preferences-synchronizer": "0.0.1",
    "@eclipse-che/che-theia-hosted-plugin-manager-extension": "0.0.1",
    "@eclipse-che/theia-dashboard-extension": "0.0.1",
    "@eclipse-che/theia-activity-tracker": "1.0.0",
    "@eclipse-che/theia-about": "0.0.1",
    "@eclipse-che/theia-terminal": "0.0.1",
    "@eclipse-che/theia-preferences-provider-extension": "0.0.1",
    "@eclipse-che/theia-git-provisioner": "0.0.1",
    "@eclipse-che/theia-logging": "1.0.0",
    "@eclipse-che/theia-messaging": "0.0.1",
    "@eclipse-che/theia-mini-browser": "0.0.1",
    "@eclipse-che/theia-file-sync-tracker": "0.0.1",
    "@eclipse-che/theia-cli-endpoint": "0.0.1",
    "@eclipse-che/theia-workspace-extension": "0.0.1",
    "@eclipse-che/theia-remote-api": "0.0.1",
    "@eclipse-che/theia-remote-impl-che-server": "0.0.1",
    "@eclipse-che/theia-remote-impl-k8s": "0.0.1",
    "@eclipse-che/theia-credentials": "0.0.1"
  },
  "bin": {
    "override-vs-loader": "./scripts/override-vs-loader.js"
  },
  "scripts": {
    "clean": "theia clean && rimraf errorShots",
    "compile": "tsc -b",
    "bundle": "theia build --mode production --config cdn/webpack.config.js --env cdn=./cdn.json --env monacopkg=@theia/monaco-editor-core@1.67.2 && yarn run override-vs-loader",
    "build": "yarn compile && yarn bundle",
    "override-vs-loader": "override-vs-loader",
    "watch": "concurrently -n compile,bundle \"theiaext watch --preserveWatchOutput\" \"theia build --watch --mode development\"",
    "start": "theia start",
    "start:debug": "yarn start --log-level=debug",
    "test": "wdio wdio.conf.js",
    "test-non-headless": "wdio wdio-non-headless.conf.js",
    "coverage:compile": "yarn build --config coverage-webpack.config.js",
    "coverage:remap": "remap-istanbul -i coverage/coverage.json -o coverage/coverage-final.json --exclude 'frontend/index.js' && rimraf coverage/coverage.json",
    "coverage:report:html": "istanbul report --root coverage --format html",
    "coverage:report:lcov": "istanbul report --root coverage --format lcov",
    "coverage": "yarn coverage:compile && yarn test && yarn coverage:remap && yarn coverage:report:lcov && yarn coverage:report:html"
  },
  "devDependencies": {
    "@theia/cli": "^1.29.0",
    "html-webpack-plugin": "^5.3.1",
    "@theia/compression-webpack-plugin": "^3.0.0",
    "circular-dependency-plugin": "^5.2.2",
    "copy-webpack-plugin": "^8.1.1",
    "webpack-cli": "^4.7.0",
    "process": "^0.11.10"
  }
}